programa
{
	
	funcao inicio()
	{
		inteiro numero

		escreva ("Até qual número você deseja contar? \n")
		leia (numero)

		para (inteiro i = 0; i <= numero; i++)
		{
			escreva (i, "\n")
		}
	}
}
